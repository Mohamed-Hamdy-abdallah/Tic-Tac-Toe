            <Image url="@images/welcome.gif" />
         </image>
      </ImageView>
      <Label fx:id="username" alignment="CENTER" contentDisplay="CENTER" layoutX="14.0" layoutY="99.0" prefHeight="112.0" prefWidth="583.0" text="Username" textAlignment="CENTER" wrapText="true">
